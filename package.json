{
  "name": "studio-starter-template",
  "version": "1.0.0",
  "description": "Studio Starter Template Uses Webpack, Browsersync, Navigo for Routing and Node / Express for deployment",
  "scripts": {
    "start": "nodemon server/app.js",
    "serve": "node node_modules/gulp/bin/gulp serve",
    "build": "node node_modules/gulp/bin/gulp build",
    "build-server": "node node_modules/gulp/bin/gulp build-server",
    "build-all": "node node_modules/gulp/bin/gulp clean-dist && npm run build && npm run build-server",
    "forever-start": "node node_modules/forever/bin/forever start server/app.js",
    "forever-stop": "node node_modules/forever/bin/forever stop server/app.js",
    "start-prod": "npm run forever-start",
    "launch": "npm run build && npm run start-prod"
  },
  "repository": {
    "type": "git",
    "url": "git+http://10.25.44.146/bberzellini/studio-starter-template"
  },
  "license": "MIT",
